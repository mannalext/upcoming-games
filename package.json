{
  "name": "upcoming-games",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve --mode dev --port 8090",
    "build": "vue-cli-service build",
    "test": "concurrently -p '路{name}路' -n UNIT,LINT -c cyan.inverse,white.inverse 'npm:test:unit' 'npm:lint'",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "test:uat:dev": "npx cypress run --env environment=dev",
    "lint": "concurrently -p '路{name}路' -n CODE,COMMITS -c yellow.inverse,green.inverse 'npm:lint:code' 'npm:lint:commits'",
    "lint:code": "eslint --ext .tsx,.ts,.vue .",
    "lint:commits": "commitlint -f $(git log --reverse --pretty=%H | head -1) -t $(git log -1 --pretty=%H)",
    "lint:fix": "vue-cli-service lint",
    "update:dependencies": "npx npm-check-updates -u && npm i"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.9.0",
    "env-var": "^7.0.0",
    "vue": "^3.0.0",
    "vue-axios": "^3.2.4",
    "vue-loader": "^16.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/cli-plugin-babel": "~4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-router": "~4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-plugin-vuex": "~4.5.11",
    "@vue/cli-service": "~4.5.11",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "chromedriver": "88",
    "concurrently": "^6.0.0",
    "cypress": "^7.2.0",
    "eslint": "6.2.0",
    "eslint-plugin-vue": "^7.6.0",
    "geckodriver": "^1.22.1",
    "jest": "^26.6.3",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5",
    "vue-jest": "^5.0.0-0",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.12"
  }
}
